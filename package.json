{
  "name": "react-quill-spell-checker",
  "version": "1.4.1",
  "description": "Integrate any spell checker on Quill.js editor",
  "author": "Anderson Filho",
  "license": "MIT",
  "repository": "andersoncoder/react-quill-spell-checker",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c --bundleConfigAsCjs && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist",
    "start": "rollup -c -w --bundleConfigAsCjs",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "nanohtml": "^1.10.0",
    "quill-delta": "^5.1.0"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/runtime": "^7.22.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@rollup/plugin-url": "^8.0.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.3",
    "@types/quill": "^2.0.10",
    "@types/react": "^16.9.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^5.0.0",
    "quill": "^1.3.7",
    "react": "^16.9.0",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^16.9.0",
    "rollup": "^3.26.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.6.0",
    "typescript": "^4.1.6",
    "util": "^0.12.5"
  }
}
